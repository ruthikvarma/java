import java.io.*;
import java.util.*;
class Animal
{
    Animal(){
        System.out.println("Animal is created");
    }
}
class Dog extends Animal
{
    Dog()
    {
        super();
        System.out.println("dog is created");
    }
}
public class Main{
    public static void main (String args[])
    {
        Animal a = new Animal();
        Dog d = new Dog();
    }
}
